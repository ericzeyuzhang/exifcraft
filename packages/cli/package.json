{
  "name": "exifcraft-cli",
  "version": "1.0.0",
  "description": "CLI interface for AI-powered EXIF metadata crafting",
  "main": "dist/index.js",
  "bin": {
    "exifcraft": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "package": "npm run build && npm run package:bun-mac && npm run package:bun-win",
    "package:bun-mac": "~/.bun/bin/bun build dist/cli.js --compile --outfile dist/bin/mac/exifcraft",
    "package:bun-win": "BUN_COMPILE_TARGET=bun-windows-x64 ~/.bun/bin/bun build dist/cli.js --compile --outfile dist/bin/win/exifcraft.exe",
    "clean": "rm -rf dist",
    "test": "vitest run --config ./vitest.e2e.config.ts",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "demo": "./scripts/demo.sh",
    "demo-dry": "./scripts/demo.sh --dry-run"
  },
  "keywords": [
    "exif",
    "metadata",
    "ai",
    "image",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "exifcraft-core": "file:../core"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
