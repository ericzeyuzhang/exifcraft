{
  "name": "exifcraft",
  "version": "1.0.0",
  "description": "AI-powered EXIF metadata crafting tool for images",
  "main": "dist/index.js",
  "bin": {
    "exifcraft": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/cli.js",
    "dev": "ts-node src/bin/cli.ts",
    "clean": "rm -rf dist",
    "demo": "./scripts/demo.sh",
    "demo-dry": "./scripts/demo-dry.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts"
  },
  "keywords": [
    "exif",
    "metadata",
    "ai",
    "image",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "exiftool-vendored": "^30.3.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
