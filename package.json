{
  "name": "exifcraft-monorepo",
  "version": "1.0.0",
  "description": "AI-powered EXIF metadata crafting tool - Monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces",
    "dev:cli": "npm run dev --workspace=exifcraft-cli",
    "build:core": "npm run build --workspace=exifcraft-core",
    "build:cli": "npm run build --workspace=exifcraft-cli"
  },
  "keywords": [
    "exif",
    "metadata",
    "ai",
    "image",
    "cli",
    "monorepo"
  ],
