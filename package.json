{
  "name": "exifcraft",
  "version": "1.0.0",
  "description": "AI-powered EXIF metadata crafting tool for images",
  "main": "dist/index.js",
  "bin": {
    "exifcraft": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/cli.js",
    "dev": "ts-node src/bin/cli.ts",
    "dev:demo": "ts-node src/bin/cli.ts -d ./tests/images/demo -c ./config.ts --verbose",
    "dev:demo:dry": "ts-node src/bin/cli.ts -d ./tests/images/demo -c ./config.ts --verbose --dry-run",
    "clean": "rm -rf dist",
    "test:demo": "npm run build && node dist/bin/cli.js -d ./tests/images/demo -c ./config.ts --verbose",
    "test:demo:dry": "npm run build && node dist/bin/cli.js -d ./tests/images/demo -c ./config.ts --verbose --dry-run"
  },
  "keywords": [
    "exif",
    "metadata",
    "ai",
    "image",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "exiftool-vendored": "^30.3.0",
    "glob": "^10.3.0",
    "heic-convert": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "overrides": {
    "glob": "^10.3.0"
  }
}
